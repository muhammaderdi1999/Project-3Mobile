<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_vertical"
    android:orientation="vertical"
    android:background="@color/greendark">
<!--BELUM DI EDIT KARENA HARUS NAMBAHIN LOGO-->
    <ImageView
        android:id="@+id/logodisporabudpar"
        android:layout_width="wrap_content"
        android:layout_gravity="center"
        android:layout_height="@dimen/_120sdp"
        android:layout_marginBottom="@dimen/_minus40sdp"
        android:src="@drawable/logodisporabudpar" />

    <TextView
        android:id="@+id/text1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="DINAS KEPEMUDAAN, OLAHRAGA, KEBUDAYAAN DAN PARIWISATA"
        android:textColor="@color/white"
        android:textAlignment="center"
        android:layout_margin="@dimen/_50sdp"
        android:textSize="@dimen/_12ssp"
        android:maxLines="2"
        android:gravity="center_horizontal"/>
    <TextView
        android:id="@+id/text2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="KABUPATEN NGANJUK"
        android:textColor="@color/white"
        android:textAlignment="center"
        android:textSize="@dimen/_20ssp"
        android:textStyle="bold"
        android:maxLines="1"
        android:layout_marginTop="@dimen/_minus45sdp"
        android:gravity="center_horizontal"/>
</LinearLayout>
