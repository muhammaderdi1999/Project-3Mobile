<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context=".MenuFragment.StatusSelesai">

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/cardditerima"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:strokeColor="@color/black"
        app:strokeWidth="@dimen/_1sdp"

        android:backgroundTint="@color/white"
        android:layout_marginStart="@dimen/_20sdp"
        android:layout_marginEnd="@dimen/_20sdp"
        android:padding="@dimen/_10sdp"
        android:layout_marginTop="@dimen/_20sdp"
        app:cardElevation="@dimen/_10sdp"
        app:cardMaxElevation="@dimen/_20sdp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="@dimen/_15sdp"
            android:gravity="left">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginLeft="@dimen/_10sdp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Pinjam Tempat"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:textSize="@dimen/_16ssp"/>
                <TableLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    >

                    <TableRow>
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Tanggal "
                                android:textColor="@color/black"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:textSize="@dimen/_12ssp"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Nama "
                                android:textColor="@color/black"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:textSize="@dimen/_12ssp"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=" : "
                                android:textColor="@color/black"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:textSize="@dimen/_12ssp"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=" : "
                                android:textColor="@color/black"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:textSize="@dimen/_12ssp"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <TextView
                                android:id="@+id/tanggalstatus1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="19 September 2023"
                                android:textColor="@color/black"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:textSize="@dimen/_12ssp"/>
                            <TextView
                                android:id="@+id/namastatus1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Fadillah wahyu nugraha"
                                android:textColor="@color/black"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:textSize="@dimen/_12ssp"/>
                        </LinearLayout>
                    </TableRow>
                </TableLayout>
                <androidx.cardview.widget.CardView
                    android:layout_width="@dimen/_90sdp"
                    android:layout_height="@dimen/_20sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_gravity="bottom|right"
                    android:elevation="@dimen/_10sdp"
                    app:cardCornerRadius="@dimen/_5sdp"
                    android:backgroundTint="@color/green">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Diterima"
                        android:textColor="@color/white"
                        android:fontFamily="sans-serif"
                        android:layout_gravity="center"
                        android:textSize="@dimen/_14ssp"
                        android:textStyle="bold"/>
                </androidx.cardview.widget.CardView>
            </LinearLayout>
        </LinearLayout>
    </com.google.android.material.card.MaterialCardView>

</LinearLayout>